INSERT INTO dicionario (id, idioma, definicao, existe) VALUES (8269, 'HEBRAICO', '(śar) de 8323; um dirigente (de qualquer nível ou classe) :-- chefe, capitão, general, governador, guardião, senhor, maioral, feitor, príncipe, principal, líder, mordomo.
