INSERT INTO dicionario (id, idioma, definicao, existe) VALUES (8549, 'HEBRAICO', '(tãmîm) de 8552; inteiro (literal, figurado ou moral); também (como substantivo) integridade, verdade :-- sem mancha, sem defeito, inteiro, perfeito, (em) sinceridade, sinceramente, (com) integridade, reto, irrepreensível, sincero, inculpável.
